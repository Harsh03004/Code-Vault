{
  "name": "code-vault",
  "version": "1.0.0",
  "description": "A secure, client-side encryption vault with an AI-powered code assistant",
  "main": "index.js",
"scripts": {
  "compile:wasm": "emcc ./cpp_encryption/encryption_wrapper.cpp -o ./frontend/vault.js -s WASM=1 -s EXPORTED_RUNTIME_METHODS=\"['cwrap']\" -O3",
  "start:backend": "nodemon ./backend/server.js",
  "start:frontend": "serve ./frontend -l 5000",
  "dev": "concurrently \"npm:start:backend\" \"npm:start:frontend\""
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harsh03004/Code-Vault.git"
  },
  "keywords": [],
  "author": "",
