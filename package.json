  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Harsh03004/Code-Vault/issues"
  },
  "homepage": "https://github.com/Harsh03004/Code-Vault#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "serve": "^14.2.4"
  }
}
